CREATE DATABASE 'nswrep';
CREATE USER 'nswadmin'@'localhost' IDENTIFIED BY 'nswpassword1*';
GRANT ALL PRIVILEGES ON *.* TO 'nswadmin'@'localhost' IDENTIFIED BY 'nswpassword1*';


